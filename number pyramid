
public class Main
{
	public static void main(String[] args) {
	    int n=6;
	    int m=5;
	    for(int i=1;i<=n;i++)
	    {
	        int spaces=n-i;
	        for(int j=0;j<=spaces;j++)
	        {
	            System.out.print(" ");
	        }
	        for(int j=1;j<=i;j++)
	        {
	          System.out.print(i+" ");
	        }
	        System.out.println();
	    }
	}
}

